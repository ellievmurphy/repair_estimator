{
  "expo": {
    "name": "Repair Estimator",
    "slug": "EstimatorNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/EstimatorLogo.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#F5F5F4",
    "newArchEnabled": false,
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Estimator to access your camera."
        }
      ],
      "expo-font",
      "expo-sqlite"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
